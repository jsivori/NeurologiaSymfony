<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Neurologia\FirstBundle\Entity\Antecedente" table="antecedente">
    <id name="idAntecedente" type="integer" column="id_antecedente">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idHistoriaClinica" type="integer" column="id_historia_clinica" nullable="true"/>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true"/>
    <field name="idTipoAntecedente" type="integer" column="id_tipo_antecedente" nullable="true"/>
  </entity>
</doctrine-mapping>
