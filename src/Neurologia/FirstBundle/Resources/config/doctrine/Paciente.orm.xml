<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Neurologia\FirstBundle\Entity\Paciente" table="paciente">
    <id name="idPersona" type="integer" column="id_persona">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNivelEducacional" type="integer" column="id_nivel_educacional" nullable="true"/>
    <field name="ocupacion" type="string" column="ocupacion" length="255" nullable="true"/>
    <field name="otros" type="string" column="otros" length="255" nullable="true"/>
    <field name="idObraSocial" type="integer" column="id_obra_social" nullable="true"/>
    <field name="idUsuario" type="integer" column="id_usuario" nullable="true"/>
    <field name="idDepartamento" type="integer" column="id_departamento" nullable="true"/>
  </entity>
</doctrine-mapping>
