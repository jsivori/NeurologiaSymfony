<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Neurologia\FirstBundle\Entity\Diagnostico" table="diagnostico">
    <id name="idDiagnostico" type="integer" column="id_diagnostico">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEvolucion" type="integer" column="id_evolucion" nullable="true"/>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
