<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Neurologia\FirstBundle\Entity\Imagen" table="imagen">
    <id name="idImagen" type="integer" column="id_imagen">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEstudio" type="integer" column="id_estudio" nullable="true"/>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true"/>
    <field name="archivo" type="string" column="archivo" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
